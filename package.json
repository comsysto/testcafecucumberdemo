{
  "name": "jenkins-testcafe",
  "version": "0.0.1",
  "license": "MIT",
  "jenkinsJobPath": "jenkins_home/jobs/",
  "jenkinsJobConfigPath": "src/config/jobs/",
  "scripts": {
    "testJsonChrome": "./node_modules/.bin/cucumber-js --world-parameters {\\\"browsers\\\":[\\\"chrome\\\"]\\,\\\"exampleUrl\\\":\\\"http://devexpress.github.io/testcafe/example/\\\"\\} --format json:reports/cucumber_report.json",
    "testJsonFirefox": "./node_modules/.bin/cucumber-js --world-parameters {\\\"browsers\\\":[\\\"firefox\\\"]\\,\\\"exampleUrl\\\":\\\"http://devexpress.github.io/testcafe/example\\\"\\} --format json:reports/cucumber_report.json",
    "testJsonSlow": "./node_modules/.bin/cucumber-js --world-parameters {\\\"options\\\":{\\\"speed\\\":0.5}\\,\\\"browsers\\\":[\\\"chrome\\\"]\\,\\\"exampleUrl\\\":\\\"http://devexpress.github.io/testcafe/example\\\"\\} --format json:reports/cucumber_report.json",
    "generateHtmlReport": "node src/reporters/html/htmlGenerator.js",
    "remoteJsonTest": "./node_modules/.bin/cucumber-js --world-parameters {\\\"options\\\":{\\\"remote\\\":true\\,\\\"speed\\\":0.1}\\,\\\"exampleUrl\\\":\\\"http://devexpress.github.io/testcafe/example\\\"\\} --format json:reports/cucumber_report.json",
    "e2e": "npm run testJsonChrome; npm run generateHtmlReport",
    "e2edocker": "xvfb-run --server-num=99 --server-args='-ac -screen 0 1024x768x16' npm run e2e",
    "buildJenkins": "mkdir -p $npm_package_jenkinsJobPath && cp -Ra src/config/jobs/* $npm_package_jenkinsJobPath && docker build -t jenkinstescafe .",
    "startJenkins": "_pwd=\"$(pwd)\" && docker run -d -p 8080:8080 -p 50000:50000 -v $_pwd/jenkins_home:/var/jenkins_home jenkinstescafe:latest",
    "stopJenkins": "docker stop $(docker ps -q --filter ancestor=jenkinstescafe)"
  },
  "bin": {
    "gherkin-testcafe": "./node_modules/.bin/gherkin-javascript"
  },
  "devDependencies": {
    "cucumber": "4.0.0",
    "testcafe": "0.20.2",
    "qrcode-terminal": "0.12.0",
    "cucumber-junit": "1.7.1",
    "cucumber-html-reporter": "4.0.2",
    "shelljs": "0.8.1",
    "html-entities": "1.2.1",
    "husky": "0.14.3",
    "object.values": "1.0.4",
    "object.keys": "0.1.0"
  }
}
